<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CenterIconButton">
        <attr name="cib_drawableTint" format="color"/>
        <attr name="cib_drawable" format="reference"/>
        <attr name="cib_text" format="string"/>
    </declare-styleable>

    <declare-styleable name="CustomTextView">
        <attr name="ctv_drawableTint" format="color"/>
    </declare-styleable>

    <declare-styleable name="GraphView">
        <attr name="android:background" />
        <attr name="gv_gridColor" format="reference" />
        <attr name="gv_isXValuesEnabled" format="boolean" />
        <attr name="gv_isRightAxisEnabled" format="boolean" />
        <attr name="gv_rightAxisMinimum" format="float" />
        <attr name="gv_rightAxisMaximum" format="float" />
        <attr name="gv_lineColor" format="reference|color" />
        <attr name="gv_lineWidth" format="dimension" />
        <attr name="gv_fillDrawable" format="reference" />
    </declare-styleable>

</resources>